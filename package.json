    "lie": "3.1.1",
    "memdown": "1.2.4",
    "pouchdb": "6.1.1",
    "rimraf": "2.6.1",
    "testem": "1.15.0",
    "underscore-node": "0.1.2"
  },
  "devDependencies": {
